<?xml version='1.0' encoding='utf-8'?>
<job-scheduling-data xmlns="http://www.quartz-scheduler.org/xml/JobSchedulingData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.quartz-scheduler.org/xml/JobSchedulingData
                    http://www.quartz-scheduler.org/xml/job_scheduling_data_1_8.xsd" version="1.8">
  <!--
  		CRON EXPRESSION : SECOND / MINUTES / HOURS / DAY OF MONTH / MONTH / DAY OF WEEK / YEAR (option)
  		doc: http://www.quartz-scheduler.org/documentation/quartz-2.x/tutorials/crontrigger.html
    -->
  <!-- *************************************************** -->
  <!-- Integration des fichiers d'adresses coordonnees : NORIA -->
  <!-- *************************************************** -->
  <schedule>
    <job>
      <name>noria-job</name>
      <group>NORIA_GROUP</group>
      <description>NORIA for Geotrack</description>
      <job-class>com.orange.geotrack.backoffice.jobs.IntegrationAdressesCoordonneesJob</job-class>
    </job>
    <trigger>
      <cron>
        <name>noria-trigger</name>
        <group>NORIA_TRIGGER_GROUP</group>
        <job-name>noria-job</job-name>
        <job-group>NORIA_GROUP</job-group>
        <!-- tous les jours a 8h ... -->
        <cron-expression>0 0 8 ? * *</cron-expression>
      </cron>
    </trigger>
  </schedule>

  <!-- ******************************* -->
  <!-- SUIVI / IMMEDIATE = SI          -->
  <!-- ******************************* -->
  <schedule>
    <job>
      <name>si-loc-job</name>
      <group>SI_GROUP</group>
      <description>SI Geotrack</description>
      <job-class>com.orange.geotrack.backoffice.jobs.SuiviImmediateLocJob</job-class>
    </job>
    <trigger>
      <cron>
        <name>si-loc-trigger</name>
        <group>SI_TRIGGER_GROUP</group>
        <job-name>si-loc-job</job-name>
        <job-group>SI_GROUP</job-group>
        <!-- Every 30 s -->
        <cron-expression>0/30 * * * * ?</cron-expression>
      </cron>
    </trigger>
  </schedule>
  <!-- ************ -->
  <!-- Statistiques -->
  <!-- ************ -->
  <schedule>
    <job>
      <name>stats-consults-hitaggregate-job</name>
      <group>STATS_CONSULTS_GROUP</group>
      <description>Aggregation des statistiques consultations hit</description>
      <job-class>com.orange.geotrack.backoffice.jobs.StatsConsultationsJob</job-class>
    </job>
    <trigger>
      <cron>
        <name>stats-consults-hitaggregate-trigger</name>
        <group>STATS_CONSULTS_TRIGGER_GROUP</group>
        <job-name>stats-consults-hitaggregate-job</job-name>
        <job-group>STATS_CONSULTS_GROUP</job-group>
		    <cron-expression>0 0/5 * * * ?</cron-expression>
          <!-- Every 5 min -->
        </cron>
    </trigger>
  </schedule>
  <schedule>
    <job>
      <name>stats-calcul-job</name>
      <group>STATS_GROUP</group>
      <description>Calcul des statistiques journalieres et mensuelles</description>
      <job-class>com.orange.geotrack.backoffice.jobs.StatistiquesCalculGenFichierJob</job-class>
    </job>
    <trigger>
      <cron>
        <name>stats-calcul-trigger</name>
        <group>STATS_TRIGGER_GROUP</group>
        <job-name>stats-calcul-job</job-name>
        <job-group>STATS_GROUP</job-group>
        <cron-expression>0 0 3 ? * *</cron-expression>
        <!-- tous les jours a 3h ... -->
      </cron>
    </trigger>
  </schedule>
  <!-- ************ -->
  <!-- Purge -->
  <!-- ************ -->
  <schedule>
    <job>
      <name>purge-bdd-job</name>
      <group>PURGE_BDD_GROUP</group>
      <description>Purge des donnees de base de donnees</description>
      <job-class>com.orange.geotrack.backoffice.jobs.PurgeBddLogHistoJob</job-class>
      <job-data-map>
        <entry>
          <key>DIR_PATH</key>
          <value>{{ rep_app_data }}/alis</value>
        </entry>
      </job-data-map>
    </job>
    <trigger>
      <cron>
        <name>purge-bdd-trigger</name>
        <group>PURGE_BDD_TRIGGER_GROUP</group>
        <job-name>purge-bdd-job</job-name>
        <job-group>PURGE_BDD_GROUP</job-group>
        <cron-expression>0 0 12 ? * *</cron-expression>
        <!-- tous les jours a 12h ... -->
      </cron>
    </trigger>
  </schedule>
  <!-- ************ -->
  <!-- Purge HIT MS20 -->
  <!-- ************ -->
  <schedule>
    <job>
      <name>purge-bdd-hit-ms20-job</name>
      <group>PURGE_BDD_HIT_MS20_GROUP</group>
      <description>Purge des donnees de hit ms20 de base de donnees</description>
      <job-class>com.orange.geotrack.backoffice.jobs.PurgeBddLocSurDemandeHitJob</job-class>
    </job>
    <trigger>
      <cron>
        <name>purge-bdd-hit-ms20-trigger</name>
        <group>PURGE_BDD_HIT_MS20_TRIGGER_GROUP</group>
        <job-name>purge-bdd-hit-ms20-job</job-name>
        <job-group>PURGE_BDD_HIT_MS20_GROUP</job-group>
        <cron-expression>0 0 0/2 * * ?</cron-expression>
        <!-- Every 2 hours -->
      </cron>
    </trigger>
  </schedule>
  <!-- ************ -->
  <!-- STATS ES ACTIV DEMAND -->
  <!-- ************ -->
  <schedule>
    <job>
      <name>stats-es-activ-demand-job</name>
      <group>STATS_ES_ACTIV_DEMAND_GROUP</group>
      <description>sauvegarde du nombre de demande d es activ</description>
      <job-class>com.orange.geotrack.backoffice.jobs.StatsESActivDemandJob</job-class>
    </job>
    <trigger>
      <cron>
        <name>stats-es-activ-demand-trigger</name>
        <group>STATS_ES_ACTIV_DEMAND_GROUP</group>
        <job-name>stats-es-activ-demand-job</job-name>
        <job-group>STATS_ES_ACTIV_DEMAND_GROUP</job-group>
        <!-- tous les premiers du mois a 2h -->
        <cron-expression>* * 2 1 * ?</cron-expression>
      </cron>
    </trigger>
  </schedule>
  <!-- **** -->
  <!-- ALIS -->
  <!-- **** -->
  <schedule>
    <!-- ALIS TrackHI2  -->
    <job>
      <name>alis-trackhi2-job</name>
      <group>ALIS_GROUP</group>
      <description>ALIS TrackHI2: abonnements et desabonnements SDK SOLO</description>
      <job-class>com.orange.geotrack.backoffice.jobs.AlisTrackHI2Job</job-class>
    </job>
    <trigger>
      <cron>
        <name>alis-trackhi2-trigger</name>
        <group>ALIS_TRIGGER_GROUP</group>
        <job-name>alis-trackhi2-job</job-name>
        <job-group>ALIS_GROUP</job-group>
        <!-- Every min -->
        <cron-expression>0 0/1 * * * ?</cron-expression>
      </cron>
    </trigger>
  </schedule>
  <schedule>
    <!-- ALIS HI2Loc-->
    <job>
      <name>alis-hi2-loc-job</name>
      <group>ALIS_GROUP</group>
      <description>ALIS HI2LocJob: traitement des localisations actives HI2</description>
      <job-class>com.orange.geotrack.backoffice.jobs.AlisHI2LocJob</job-class>
    </job>
    <trigger>
      <cron>
        <name>alis-hi2-loc-trigger</name>
        <group>ALIS_TRIGGER_GROUP</group>
        <job-name>alis-hi2-loc-job</job-name>
        <job-group>ALIS_GROUP</job-group>
        <!-- Every 20 s -->
        <cron-expression>15/30 * * * * ?</cron-expression>
      </cron>
    </trigger>
  </schedule>
  <schedule>
    <!-- ALIS AlisHI2ResendFileJob -->
    <job>
      <name>alis-hi2-resend-file-job</name>
      <group>ALIS_GROUP</group>
      <description>ALIS HI2ResendFileJob : renvoi des fichiers hi2 cote diode et cote geotrack</description>
      <job-class>com.orange.geotrack.backoffice.jobs.AlisHI2ResendFileJob</job-class>
      <job-data-map>
        <entry>
          <key>DIR_PATH</key>
          <value>{{ rep_app_data }}/alis</value>
        </entry>
      </job-data-map>
    </job>
    <trigger>
      <cron>
        <name>alis-hi2-resend-file-trigger</name>
        <group>ALIS_TRIGGER_GROUP</group>
        <job-name>alis-hi2-resend-file-job</job-name>
        <job-group>ALIS_GROUP</job-group>
        <!-- Evry 15 min -->
        <cron-expression>0 0/15 * * * ?</cron-expression>
      </cron>
    </trigger>
  </schedule>
  <schedule>
    <!-- ALIS HI2FileGenerate -->
    <job>
      <name>alis-hi2-filegenerate-job</name>
      <group>ALIS_GROUP</group>
      <description>ALIS HI2FileGenerate: generation des fichiers ALIS pour demandes HI2</description>
      <job-class>com.orange.geotrack.backoffice.jobs.AlisHI2FileGenerate</job-class>
      <job-data-map>
        <entry>
          <key>DIR_PATH</key>
          <value>{{ rep_app_data }}/alis</value>
        </entry>
        <entry>
          <key>FILENAME_FORMAT</key>
          <value>HIT_%s.hit</value>
        </entry>
      </job-data-map>
    </job>
    <trigger>
      <cron>
        <name>alis-hi2-filegenerate-trigger</name>
        <group>ALIS_TRIGGER_GROUP</group>
        <job-name>alis-hi2-filegenerate-job</job-name>
        <job-group>ALIS_GROUP</job-group>
        <!-- Evry min -->
        <cron-expression>0 0/1 * * * ?</cron-expression>
      </cron>
    </trigger>
  </schedule>
  <schedule>
    <!-- ALIS THI2FileGenerate -->
    <job>
      <name>alis-thi2-filegenerate-job</name>
      <group>ALIS_GROUP</group>
      <description>ALIS THI2FileGenerate: generation des fichiers ALIS pour demandes Tracking HI2</description>
      <job-class>com.orange.geotrack.backoffice.jobs.AlisTHI2FileGenerate</job-class>
      <job-data-map>
        <entry>
          <key>DIR_PATH</key>
          <value>{{ rep_app_data }}/alis</value>
        </entry>
        <entry>
          <key>FILENAME_FORMAT</key>
          <value>EVENT_%s.event</value>
        </entry>
      </job-data-map>
    </job>
    <trigger>
      <cron>
        <name>alis-thi2-filegenerate-trigger</name>
        <group>ALIS_TRIGGER_GROUP</group>
        <job-name>alis-thi2-filegenerate-job</job-name>
        <job-group>ALIS_GROUP</job-group>
        <!-- Every min -->
        <cron-expression>0 0/1 * * * ?</cron-expression>
      </cron>
    </trigger>
  </schedule>
  <schedule>
    <!-- HORODATAGE FileGenerate -->
    <job>
      <name>horodatage-filegenerate-job</name>
      <group>HORODATAGE_GROUP</group>
      <description>HORODATAGE FileGenerate: generation des fichiers d'horodatage</description>
      <job-class>com.orange.geotrack.backoffice.jobs.HorodatageFileGenerate</job-class>
      <job-data-map>
        <entry>
          <key>DIR_PATH</key>
          <value>{{ rep_app_data }}/horodatage/out/</value>
        </entry>
        <entry>
          <key>FILENAME_FORMAT</key>
          <value>HORODATAGE_%s.csv</value>
        </entry>
      </job-data-map>
    </job>
    <trigger>
      <cron>
        <name>horodatage-filegenerate-trigger</name>
        <group>HORODATAGE_TRIGGER_GROUP</group>
        <job-name>horodatage-filegenerate-job</job-name>
        <job-group>HORODATAGE_GROUP</job-group>
        <!-- Tous les 1er du mois Ã  4h -->
        <cron-expression>* * 4 1 * ?</cron-expression>
      </cron>
    </trigger>
  </schedule>
</job-scheduling-data>
